{
    "name": "node-demo",
    "version": "0.0.0",
    "description": "Node Testing Demo",
    "main": "server.js",
    "private": true,
    "engines": {
        "node": ">=4.6.x",
        "npm": ">=2.15.x"
    },
    "scripts": {
        "dev": "nodemon --exec DEBUG=node* node_modules/babel-cli/bin/babel-node.js server"
    },
    "dependencies": {
        "body-parser": "1.15.2",
        "compression": "1.6.2",
        "cookie-parser": "1.4.3",
        "cors": "2.8.1",
        "debug": "^2.2.0",
        "etsy-js": "0.0.6",
        "express": "4.14.0",
        "express-jwt": "5.1.0",
        "express-validation": "1.0.1",
        "express-winston": "2.0.0",
        "faker": "3.1.0",
        "helmet": "3.1.0",
        "http-status": "^0.2.0",
        "joi": "9.2.0",
        "jsonwebtoken": "7.1.9",
        "lodash": "4.17.4",
        "method-override": "^2.3.5",
        "mongodb": "^2.2.28",
        "mysql2": "1.4.2",
        "parse-url": "3.0.2",
        "q": "^1.5.1",
        "sequelize": "^4.28.0",
        "winston": "2.3.0"
    },
    "devDependencies": {
        "babel-cli": "6.18.0",
        "babel-core": "6.18.0",
        "babel-plugin-add-module-exports": "0.2.1",
        "babel-plugin-transform-object-assign": "6.8.0",
        "babel-preset-latest": "^6.16.0",
        "eslint": "3.10.2",
        "eslint-config-airbnb-base": "7.1.0",
        "eslint-plugin-import": "1.16.0",
        "eslint-watch": "2.1.14",
        "gulp-nodemon": "^2.0.6"
    },
    "config": {
        "ghooks": {
            "pre-commit": "npm run lint && npm test"
        },
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "babel": {
        "presets": [
            "latest"
        ],
        "plugins": [
            "add-module-exports",
            "transform-object-assign"
        ]
    }
}